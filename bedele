// pirmas variantas

function autoFunkcija(a, b, c) {
  const autoSalonas = `Auto salone galima isigyti nauju ${a}, devetu ${b} ir senu ${c} masinu.`;
  return autoSalonas;
}

const finisas = autoFunkcija("audi", "bmw", "mersas");

console.log(finisas);


// antras variantas

function autoFunkcija2(a, b, c) {
  const autoSalonas2 = `Auto salone galima isigyti nauju ${a}, devetu ${b} ir senu ${c} masinu.`;
  console.log(autoSalonas2);
}

autoFunkcija2("audi", "bmw", "mersas");



